#!/bin/zsh

alias gs='git status'
alias gsw='git switch'
alias gl='git log --oneline'
alias gpl='git pull'
alias gd='git diff'
alias gp='git push'
alias gpf='git push --force-with-lease'
alias ga='gs; git add . ;gs'
alias gc='git commit -m'
alias gck='git checkout -b'
alias gf='git fetch'
alias gfo='git fetch origin'
alias gr='git rebase'
